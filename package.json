{
  "name": "monaco-review",
  "version": "0.0.39",
  "description": "Code review extension for Monaco",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "prettier": {
    "printWidth": 120
  },
  "scripts": {
    "start": "webpack serve",
    "compile": "tsc && webpack --mode=production",
    "tsc": "tsc --watch",
    "doc": "typedoc --out ./docs ./src/index.ts",
    "test": "jest --coverage",
    "testpub": "jest --coverage --coverageReporters=text-lcov | coveralls"
  },
  "author": "James Burrow",
  "license": "MIT",
  "peerDependencies": {
    "monaco-editor": ">0.32"
  },
  "dependencies": {
    "showdown": "^2.1.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "jest-environment-jsdom": "29.7.0",
    "ts-jest": "29.1.1",
    "@babel/core": "7.23.0",
    "@babel/preset-env": "7.22.20",
    "@babel/preset-typescript": "7.23.0",
    "@babel/runtime": "7.23.1",
    "@babel/types": "7.23.0",
    "@types/jest": "29.5.5",
    "@types/uuid": "9.0.5",
    "@webpack-cli/serve": "1.7.0",
    "babel-jest": "29.7.0",
    "coveralls": "3.1.1",
    "dayjs": "1.11.10",
    "moment": "2.29.4",
    "monaco-editor": "0.44.0",
    "ts-loader": "9.5.0",
    "typedoc": "0.25.1",
    "typescript": "5.2.2",
    "webpack": "5.88.2",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.15.1"
  }
}
