{
  "name": "monaco-review",
  "version": "0.0.39",
  "description": "Code review extension for Monaco",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "prettier": {
    "printWidth": 120
  },
  "scripts": {
    "start": "webpack serve",
    "compile": "tsc && webpack --mode=production",
    "tsc": "tsc --watch",
    "doc": "typedoc --out ./docs ./src/index.ts",
    "test": "jest --coverage",
    "testpub": "jest --coverage --coverageReporters=text-lcov | coveralls"
  },
  "author": "James Burrow",
  "license": "MIT",
  "peerDependencies": {
    "monaco-editor": ">0.32"
  },
  "dependencies": {
    "showdown": "^2.1.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "jest-environment-jsdom": "29.2.2",
    "ts-jest": "29.0.3",
    "@babel/core": "7.19.6",
    "@babel/preset-env": "7.19.4",
    "@babel/preset-typescript": "7.18.6",
    "@babel/runtime": "7.20.1",
    "@babel/types": "7.20.0",
    "@types/jest": "29.2.1",
    "@types/uuid": "8.3.4",
    "@webpack-cli/serve": "1.7.0",
    "babel-jest": "29.2.2",
    "coveralls": "3.1.1",
    "dayjs": "1.11.6",
    "moment": "2.29.4",
    "monaco-editor": "0.34.1",
    "ts-loader": "9.4.1",
    "typedoc": "0.23.20",
    "typescript": "4.8.4",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.11.1"
  }
}
