{
  "name": "monaco-review",
  "version": "0.0.28",
  "description": "Code review extension for Monaco",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "prettier": {
    "printWidth": 120
  },
  "scripts": {
    "start": "webpack serve",
    "compile": "tsc && webpack --mode=production",
    "tsc": "tsc --watch",
    "doc": "typedoc --out ./docs ./src/index.ts",
    "test": "jest --coverage",
    "testpub": "jest --coverage --coverageReporters=text-lcov | coveralls"
  },
  "author": "James Burrow",
  "license": "MIT",
  "peerDependencies": {
    "monaco-editor": "0.23||0.24||0.25"
  },
  "dependencies": {
    "showdown": "^1.9.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/runtime": "^7.12.5",
    "@babel/types": "^7.12.12",
    "@types/jest": "^26.0.23",
    "@types/uuid": "^8.3.0",
    "@webpack-cli/serve": "^1.2.2",
    "coveralls": "^3.1.0",
    "dayjs": "^1.10.4",
    "jest": "^27.0.4",
    "moment": "^2.29.1",
    "monaco-editor": "0.25.2",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "typedoc": "^0.21.0",
    "typescript": "^4.3.4",
    "webpack": "^5.39.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  }
}
