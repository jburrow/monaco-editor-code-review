{
  "name": "monaco-review",
  "version": "0.0.37",
  "description": "Code review extension for Monaco",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "prettier": {
    "printWidth": 120
  },
  "scripts": {
    "start": "webpack serve",
    "compile": "tsc && webpack --mode=production",
    "tsc": "tsc --watch",
    "doc": "typedoc --out ./docs ./src/index.ts",
    "test": "jest --coverage",
    "testpub": "jest --coverage --coverageReporters=text-lcov | coveralls"
  },
  "author": "James Burrow",
  "license": "MIT",
  "peerDependencies": {
    "monaco-editor": ">0.32"
  },
  "dependencies": {
    "showdown": "^2.0.0-alpha",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/runtime": "7.17.8",
    "@babel/types": "^7.17.0",
    "@types/jest": "^27.4.0",
    "@types/uuid": "^8.3.4",
    "@webpack-cli/serve": "^1.6.1",
    "coveralls": "^3.1.1",
    "dayjs": "^1.10.7",
    "jest": "^27.4.7",
    "moment": "^2.29.1",
    "monaco-editor": "^0.33.0",
    "ts-jest": "27.1.5",
    "ts-loader": "^9.2.6",
    "typedoc": "^0.22.11",
    "typescript": "^4.5.5",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  }
}
