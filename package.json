{
  "name": "monaco-review",
  "version": "0.0.38",
  "description": "Code review extension for Monaco",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "prettier": {
    "printWidth": 120
  },
  "scripts": {
    "start": "webpack serve",
    "compile": "tsc && webpack --mode=production",
    "tsc": "tsc --watch",
    "doc": "typedoc --out ./docs ./src/index.ts",
    "test": "jest --coverage",
    "testpub": "jest --coverage --coverageReporters=text-lcov | coveralls"
  },
  "author": "James Burrow",
  "license": "MIT",
  "peerDependencies": {
    "monaco-editor": ">0.32"
  },
  "dependencies": {
    "showdown": "^2.1.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "jest-environment-jsdom": "^28.1.3",
    "ts-jest": "^28.0.6",
    "@babel/core": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/runtime": "7.18.6",
    "@babel/types": "^7.18.8",
    "@types/jest": "28.1.6",
    "@types/uuid": "^8.3.4",
    "@webpack-cli/serve": "^1.6.1",
    "babel-jest": "^28.1.3",
    "coveralls": "^3.1.1",
    "dayjs": "^1.11.3",
    "moment": "^2.29.1",
    "monaco-editor": "^0.33.0",
    "ts-loader": "^9.3.1",
    "typedoc": "^0.23.7",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3"
  }
}
