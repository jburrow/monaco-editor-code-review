{
  "name": "monaco-review",
  "version": "0.0.22",
  "description": "Code review extension for Monaco",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "prettier": {
    "printWidth": 120
  },
  "scripts": {
    "start": "webpack-dev-server",
    "compile": "tsc && webpack --mode=development",
    "tsc": "tsc",
    "doc": "typedoc --out ./docs ./src/index.ts",
    "test": "jest --coverage",
    "testpub": "jest --coverage --coverageReporters=text-lcov | coveralls"
  },
  "author": "James Burrow",
  "license": "MIT",
  "peerDependencies": {
    "monaco-editor": ">=0.19"
  },
  "dependencies": {
    "showdown": "^1.9.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "css-loader": "^5.0.1",
    "style-loader": "^2.0.0",
    "dayjs": "^1.10.4",
    "@babel/runtime": "^7.12.5",
    "@babel/types": "^7.12.12",
    "@types/jest": "^26.0.20",
    "@types/uuid": "^8.3.0",
    "@webpack-cli/serve": "^1.2.2",
    "coveralls": "^3.1.0",
    "jest": "^26.6.3",
    "moment": "^2.29.1",
    "monaco-editor": "^0.21.3",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.14",
    "typedoc": "^0.20.16",
    "typescript": "^4.1.3",
    "webpack": "^4.44.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
